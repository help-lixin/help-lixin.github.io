<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>protected EventLoop newChild(Executor executor, Object... args) throws Exception {
    // args = [
    // sun.nio.ch.KQueueSelectorProvider
    // io.netty.channel.DefaultSelectStrategyFactory
    // io.netty.util.concurrent.RejectedExecutionHandlers$1
    // ]
    EventLoopTaskQueueFactory queueFactory = args.length == 4 ? (EventLoopTaskQueueFactory) args[3] : null;
    return new NioEventLoop( 
           // NioEventLoopGroup 
           this, 
           // io.netty.util.concurrent.ThreadPerTaskExecutor
           executor, 
           // sun.nio.ch.KQueueSelectorProvider
           (SelectorProvider) args[0],
           // io.netty.channel.DefaultSelectStrategyFactory
           ((SelectStrategyFactory) args[1]).newSelectStrategy(), 
           // io.netty.util.concurrent.RejectedExecutionHandlers$1
           (RejectedExecutionHandler) args[2], 
           // null
           queueFactory
    ); //end create NioEventLoop
}
